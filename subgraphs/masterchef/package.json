{
    "name": "masterchef",
    "license": "MIT",
    "version": "1.0.0",
    "author": "SushiSwap",
    "scripts": {
        "prepare:avax": "mustache config/avax.json masterchef.template.yaml > masterchef.avax.yaml",
        "prepare:fuji": "mustache config/fuji.json masterchef.template.yaml > masterchef.fuji.yaml",

        "codegen:avax": "graph codegen masterchef.avax.yaml",
        "codegen:fuji": "graph codegen masterchef.fuji.yaml",

        "build:avax": "graph build masterchef.avax.yaml",
        "build:fuji": "graph build masterchef.fuji.yaml",

        "deploy:fuji": "graph deploy --node https://api.thegraph.com/deploy/ --ipfs https://api.thegraph.com/ipfs/ 0xmurloc/joeDefiMasterChefFuji masterchef.avax.yaml",
        "deploy:avax": "graph deploy --node https://api.thegraph.com/deploy/ --ipfs https://api.thegraph.com/ipfs/ 0xmurloc/joeDefiMasterChefAvax masterchef.fuji.yaml",
        "create-local": "graph create --node http://localhost:8020/ /0xmurloc/joeDefi",
        "remove-local": "graph remove --node http://localhost:8020/ /0xmurloc/joeDefi",
        "deploy-local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 /0xmurloc/joeDefi"
    }
}
